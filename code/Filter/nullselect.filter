CodeDef nullselect Filter
CodeDef nullselect Description Combo box
CodeDef nullselect Routine <<EOR
sub {
	my @some = split /\0+/, shift;
	for(@some) {
		return $_ if length $_;
	}
	return '';
}
EOR
