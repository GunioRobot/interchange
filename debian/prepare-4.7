#! /bin/sh -e

VERSION=`dpkg-parsechangelog -lchangelog | perl -ne 'print if s/^Version: ((.*?)\.(.*?)\.(.*?))-(.*)?$/$2.$3/'`
if [ "$VERSION" != 4.7 ]; then
	echo "This script should be applied only to IC 4.7.x instead of $VERSION.x !"
    exit 1
fi

# Quickstart no longer exists
perl -i.bak -pe 's/^QuickStart$//' interchange.docs

